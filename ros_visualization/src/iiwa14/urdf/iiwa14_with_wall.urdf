<?xml version="1.0"?>
<robot name = "iiwa14">
    <!--world-->
    <link name ="world"/>

    <!-- joint between goal and world -->
    <joint name="world_goal" type="fixed">
      <origin xyz="5.06049896 0.49907388 0.80269118" rpy="0 0 0"/>
      <parent link="world"/>
      <child link="goal"/>
    </joint>

    <!-- goal-->
    <link name="goal">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.005"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    </link>
    
    <!-- attach the wall to the robot-->
    <!-- change the rpy value to "0 0 0.523", the wall will be straight-->
    <joint name="world_wall_joint" type="fixed">
      <origin xyz="5 0.42 0" rpy="0 0 0"/>
      <parent link="world"/>
      <child link="wall_link"/>
    </joint>

    <link name="wall_link">
    <visual>
      <origin xyz="0 0.02 0.7" rpy="0 0 0"/>
      <geometry>
        <box size="1 0.02 1.4"/>
      </geometry>
      <material name="gray">
        <color rgba="0.4 0.4 0.4 1"/>
      </material>
    </visual>
    </link>

    
    <!--joint between {parent} and link_0-->
    <joint name="world_iiwa14_joint" type="fixed">
      <origin xyz="5 0 0" rpy="0 0 0"/>
      <parent link="world"/>
      <child link="iiwa14_link_0"/>
    </joint>
    
    <!--link0-->
    <link name="iiwa14_link_0">
      <inertial>
        <origin xyz="-0.1 0 0.07" rpy="0 0 0"/>
        <mass value="5"/>
        <inertia ixx="0.05"  ixy="0"  ixz="0" iyy="0.06" iyz="0" izz="0.03" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://iiwa14/meshes/iiwa14/visual/link_0.stl"/>
        </geometry>
        <material name="Grey">
            <color rgba ="0.4 0.4 0.4 1.0"/>
        </material>
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://iiwa14/meshes/iiwa14/collision/link_0.stl"/>
        </geometry>
        <material name="Grey">
            <color rgba ="0.4 0.4 0.4 1.0"/>
        </material>
      </collision>
      
      <self_collision_checking>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <capsule radius="0.15" length="0.25"/>
        </geometry>
      </self_collision_checking>
      
    </link>
    
    <!-- joint between link_0 and link_1 -->
    <joint name="iiwa14_joint_1" type="revolute">
      <parent link="iiwa14_link_0"/>
      <child link="iiwa14_link_1"/>
      <origin xyz="0 0 0.1575" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="-2.96706" upper="2.96706"
             effort="300" velocity="10" />
      <safety_controller soft_lower_limit="-2.93215"
                         soft_upper_limit="2.93215"
                         k_position="100"
                         k_velocity="2"/> 
      <dynamics damping="0.5"/>
    </joint>
    
    <link name="iiwa14_link_1">
